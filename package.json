{
  "name": "qr-code-generator",
  "version": "1.0.1",
  "description": "Simple backend to generate a QR Code.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "tsc": "tsc --build tsconfig.build.json",
    "tsc:w": "tsc --build tsconfig.build.json --watch",
    "build": "npm run clean && npm run tsc",
    "build:w": "npm run clean && npm run tsc:w",
    "start": "npm run ts -- src/index.ts",
    "watch": "npm run nd -- src/index.ts",
    "ts": "ts-node",
    "nd": "nodemon --exec ts-node",
    "bs": "npm run build && node"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6.4"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.77",
    "@types/node": "^8.0.33",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "ts-jest": "24.0.1",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "^1.8.0",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "typeorm": "^0.2.30"
  }
}
